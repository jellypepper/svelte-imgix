    "imgix.js": "^3.4.2",
    "prettier": "^2.1.2",
    "query-string": "^6.13.7",
    "rollup": "^2.33.0",
    "rollup-plugin-svelte": "^6.1.0",
    "svelte": "^3.29.4",
    "svelte-preprocess": "^4.5.2",
    "svelte-types-writer": "^1.0.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
