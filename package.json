  "keywords": [
    "svelte",
    "imgix",
    "responsive-images",
    "lazyload",
    "intersection-observer"
  ],
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seaneking/svelte-imgix"
  },
  "bugs": {
    "url": "https://github.com/seaneking/svelte-imgix/issues"
  },
  "dependencies": {
    "query-string": "^6.13.7"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "prettier": "^2.1.2",
    "rollup": "^2.33.0",
    "rollup-plugin-svelte": "^6.1.0",
    "sveld": "^0.5.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  }
