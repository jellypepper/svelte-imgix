{
  "name": "svelte-imgix",
  "description": "Responsive, lazily loaded images with Svelte and Imgix",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "./Imgix.svelte",
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
