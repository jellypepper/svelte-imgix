{
  "name": "svelte-imgix",
  "description": "Responsive, lazily loaded images with Svelte and Imgix",
  "version": "0.2.5",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "svelte": "./src/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "develop": "rollup -cw",
    "typegen": "sveld",
    "prepublishOnly": "npm run build"
